{
  "name": "calendario-backend",
  "version": "1.0.0",
  "description": "Backend API para aplicaciÃ³n de calendario de gastos y quinielas",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec \"node --loader ts-node/esm\" src/server.ts",
    "dev:ts": "ts-node --esm src/server.ts",
    "simple": "node start-simple.js",
    "simple-ts": "ts-node --esm server-simple-ts.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test-real": "node test-backend-real.js",
    "test-compile": "node test-compilation.js",
    "check": "node test-quick-compile.js",
    "verify": "node verify-typescript.js",
    "fixed": "node start-backend-fixed.js",
    "fixed-esm": "node start-backend-fixed-esm.js",
    "check-imports": "node verify-imports.js",
    "lint": "eslint src/**/*.ts",
    "db:migrate": "node dist/database/migrate.js",
    "db:migrate-simple": "node src/database/migrate-simple.js",
    "compile": "tsc && node dist/server.js"
  },
  "keywords": ["express", "typescript", "calendar", "gastos", "quinielas"],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "body-parser": "^1.20.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jest": "^29.5.8",
    "@types/compression": "^1.7.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0"
  },
  "engines": {
    "node": ">=18.0.0"
